<div id="restaurant-form">
  <h3>Editing menu</h3>
  <% form_for([@restaurant,@menus]) do |f| %>
    <%= f.error_messages %>
    <p>
      <%= f.label :name %><br />
      <%= f.text_field :name %>
    </p>
    <%= hidden_field_tag :restaurant_id, @restaurant.id %>
    <p>
      <%= f.submit 'Update' %>
    </p>
  <% end %>

  <%= link_to 'Back',restaurant_path(@restaurant.id) %>

</div>